<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.dao.distributor.DistributorRateDao">

    <resultMap type="com.platform.entity.distributor.DistributorRateEntity" id="distributorRateMap">
        <result property="id" column="id"/>
        <result property="standarRate" column="standar_rate"/>
        <result property="slaerRate" column="slaer_rate"/>
        <result property="provinceRate" column="province_rate"/>
        <result property="cityRate" column="city_rate"/>
        <result property="distributorLv1S1" column="distributor_lv1_s1"/>
        <result property="distributorLv1S2" column="distributor_lv1_s2"/>
        <result property="distributorLv1S3" column="distributor_lv1_s3"/>
        <result property="distributorLv2S1" column="distributor_lv2_s1"/>
        <result property="distributorLv2S2" column="distributor_lv2_s2"/>
        <result property="distributorLv2S3" column="distributor_lv2_s3"/>
        <result property="distributorLv3S1" column="distributor_lv3_s1"/>
        <result property="distributorLv3S2" column="distributor_lv3_s2"/>
        <result property="distributorLv3S3" column="distributor_lv3_s3"/>
        <result property="status" column="status"/>
    </resultMap>

	<select id="queryObject" resultType="com.platform.entity.distributor.DistributorRateEntity">
		select
			`id`,
			`standar_rate`,
			`slaer_rate`,
			`province_rate`,
			`city_rate`,
			`distributor_lv1_s1`,
			`distributor_lv1_s2`,
			`distributor_lv1_s3`,
			`distributor_lv2_s1`,
			`distributor_lv2_s2`,
			`distributor_lv2_s3`,
			`distributor_lv3_s1`,
			`distributor_lv3_s2`,
			`distributor_lv3_s3`,
			`status`
		from distributor_rate
		where id = #{id}
	</select>

	<select id="queryList" resultType="com.platform.entity.distributor.DistributorRateEntity">
		select
    		`id`,
    		`standar_rate`,
    		`slaer_rate`,
    		`province_rate`,
    		`city_rate`,
    		`distributor_lv1_s1`,
    		`distributor_lv1_s2`,
    		`distributor_lv1_s3`,
    		`distributor_lv2_s1`,
    		`distributor_lv2_s2`,
    		`distributor_lv2_s3`,
    		`distributor_lv3_s1`,
    		`distributor_lv3_s2`,
    		`distributor_lv3_s3`,
    		`status`
		from distributor_rate
		WHERE 1=1
		<if test="name != null and name.trim() != ''">
			AND name LIKE concat('%',#{name},'%')
		</if>
        <choose>
            <when test="sidx != null and sidx.trim() != ''">
                order by ${sidx} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from distributor_rate
		WHERE 1=1
        <if test="name != null and name.trim() != ''">
            AND name LIKE concat('%',#{name},'%')
        </if>
	</select>
	 
	<insert id="save" parameterType="com.platform.entity.distributor.DistributorRateEntity" useGeneratedKeys="true" keyProperty="id">
		insert into distributor_rate(
			`standar_rate`,
			`slaer_rate`,
			`province_rate`,
			`city_rate`,
			`distributor_lv1_s1`,
			`distributor_lv1_s2`,
			`distributor_lv1_s3`,
			`distributor_lv2_s1`,
			`distributor_lv2_s2`,
			`distributor_lv2_s3`,
			`distributor_lv3_s1`,
			`distributor_lv3_s2`,
			`distributor_lv3_s3`,
			`status`)
		values(
			#{standarRate},
			#{slaerRate},
			#{provinceRate},
			#{cityRate},
			#{distributorLv1S1},
			#{distributorLv1S2},
			#{distributorLv1S3},
			#{distributorLv2S1},
			#{distributorLv2S2},
			#{distributorLv2S3},
			#{distributorLv3S1},
			#{distributorLv3S2},
			#{distributorLv3S3},
			#{status})
	</insert>
	 
	<update id="update" parameterType="com.platform.entity.distributor.DistributorRateEntity">
		update distributor_rate 
		<set>
			<if test="standarRate != null">`standar_rate` = #{standarRate}, </if>
			<if test="slaerRate != null">`slaer_rate` = #{slaerRate}, </if>
			<if test="provinceRate != null">`province_rate` = #{provinceRate}, </if>
			<if test="cityRate != null">`city_rate` = #{cityRate}, </if>
			<if test="distributorLv1S1 != null">`distributor_lv1_s1` = #{distributorLv1S1}, </if>
			<if test="distributorLv1S2 != null">`distributor_lv1_s2` = #{distributorLv1S2}, </if>
			<if test="distributorLv1S3 != null">`distributor_lv1_s3` = #{distributorLv1S3}, </if>
			<if test="distributorLv2S1 != null">`distributor_lv2_s1` = #{distributorLv2S1}, </if>
			<if test="distributorLv2S2 != null">`distributor_lv2_s2` = #{distributorLv2S2}, </if>
			<if test="distributorLv2S3 != null">`distributor_lv2_s3` = #{distributorLv2S3}, </if>
			<if test="distributorLv3S1 != null">`distributor_lv3_s1` = #{distributorLv3S1}, </if>
			<if test="distributorLv3S2 != null">`distributor_lv3_s2` = #{distributorLv3S2}, </if>
			<if test="distributorLv3S3 != null">`distributor_lv3_s3` = #{distributorLv3S3}, </if>
			<if test="status != null">`status` = #{status}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from distributor_rate where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from distributor_rate where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>